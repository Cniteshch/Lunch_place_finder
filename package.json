{
  "name": "bonify_app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://cniteshch.github.io/Lunch_place_finder",
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "gh-pages": "^2.1.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "ramda": "^0.26.1",
    "ramda-fantasy": "^0.8.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-redux-snackbar": "^1.1.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "0.9.5",
    "react-toolbox": "^2.0.0-beta.13",
    "react-toolbox-themr": "^1.0.2",
    "redux": "^4.0.4",
    "redux-form": "^8.2.6",
    "redux-thunk": "^2.3.0"
  },
  "reactToolbox": {
    "include": [
      "BUTTON",
      "DATE_PICKER",
      "TABLE",
      "TOOLTIP",
      "INPUT"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "color-accent": "var(--palette-pink-a200)",
      "color-accent-dark": "var(--palette-pink-700)",
      "color-primary-contrast": "var(--color-dark-contrast)",
      "color-accent-contrast": "var(--color-dark-contrast)"
    },
    "output": "assets/react-toolbox"
  },
  "devDependencies": {},
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "toolbox": "react-toolbox-themr"
  }
}
